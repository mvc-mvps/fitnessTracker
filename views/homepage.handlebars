<div class='card' style='width: 18rem;'>
  <img src='./img/squat.jpg' class='card-img-top' alt='...' />
  <div class='card-body'>
    <h5 class='card-title'>Daily Motivation</h5>
    <p class='card-text'>“Go the extra mile. It’s never crowded.” -Wayne Dyer</p>
  </div>
</div>

<table>
  <thead>
    <th>id</th>
    <th>Name</th>
    <th>Proteins</th>
    <th>Calories</th>
    <th>Servings</th>
  </thead>
  <tbody>
    {{#each data1 as |row|}}
      <tr>
        <td>{{row.id}}</td>
        <td>{{row.name}}</td>
        <td>{{row.protein}}</td>
        <td>{{row.calories}}</td>
        <td>{{row.serving}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

