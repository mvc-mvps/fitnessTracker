<a href="/homepage">
    <button class="btn" id="home">Home</button>
</a>

<h2>Update Food</h2>

<form>
    <div class="form-group">
        <label for="exercise-date">Time: {{this.time}}</label>
        <input class="form-control" id="food-time" placeholder="Time of day eaten">
    </div>
    <div class="form-group">
        <label for="exercise-type">Food: {{this.name}}</label>
        <input class="form-control" id="food-name" placeholder="Name of food">
    </div>
    <div class="form-group">
        <label for="food-calories">{{this.calories}} Calories </label>
        <input class="form-control" id="food-calories" placeholder="Number of calories">
    </div>
    <div class="form-group">
        <a href="/homepage"><button class="btn primary btn" id="update-food">Update</button></a>
    </div>
</form>